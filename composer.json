{
    "name": "krzysztofrewak/laravel-dashboard-nextbike-tile",
    "description": "A Nextbike tile for Laravel Dashboard",
    "keywords": [
        "spatie",
        "laravel-dashboard-velo-tile",
        "laravel-dashboard-nextbike-tile",
        "nextbike"
    ],
    "homepage": "https://github.com/krzysztofrewak/laravel-dashboard-nextbike-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Author"
        },
        {
            "name": "Krzysztof Rewak",
            "email": "krzysztof.rewak@rewak.pl",
            "homepage": "https://rewak.pl/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^7.4",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "KrzysztofRewak\\NextbikeTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KrzysztofRewak\\NextbikeTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KrzysztofRewak\\NextbikeTile\\NextbikeTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
